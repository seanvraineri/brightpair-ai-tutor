# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# OpenAI Configuration
VITE_OPENAI_API_KEY=your-openai-api-key
VITE_OPENAI_API_URL=https://api.openai.com/v1

# Application Configuration
# Set to true for production, false for development
VITE_USE_EDGE_FUNCTIONS=false

# Set to true for development when database is empty
VITE_USE_MOCK_DATA=true

# Set to true to save lessons to the database
VITE_SAVE_LESSONS=true

# AI Model Configuration
# Uncomment to override the default models in src/config/env.ts
# VITE_DEFAULT_MODEL=gpt-3.5-turbo
# VITE_PRODUCTION_MODEL=gpt-4

# Analytics (Optional)
# VITE_ENABLE_ANALYTICS=false 